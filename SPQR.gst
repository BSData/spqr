                            </constraints>
                                          
                            <rules>
                                                
                                <rule id="2556-d02d-ad15-7f68" name="Blademaster" hidden="false">
                                                      
                                    <description>Use: Passive
Prerequisite:Melee +4 or higher
His sword gains the Lethal 3 and Smasher rules.</description>
                                                    
                                </rule>
                                              
