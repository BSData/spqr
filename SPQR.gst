                    <selectionEntries>
                                    
                        <selectionEntry id="7eac-7035-8a48-d80f" name="1. Dicing with Fate" hidden="false" collective="false" type="upgrade">
                                          
                            <constraints>
                                                
                                <constraint field="selections" scope="parent" value="3.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="8705-316d-5404-79a7" type="max"/>
                                              
