                                <rule id="c370-f5a5-7c78-ec59" name="Well-Planted" hidden="false">
                                                      
                                    <description>Use: Passive
Whenever the Hero suffers a Knockdown, he may make an immediate Agility check. If successful, he ignores the Knockdown and remains standing.</description>
                                                    
                                </rule>
                                              
                            </rules>
                                          
                            <costs>
                                                
                                <cost name="Denarii" typeId="5c36-dd73-6e2a-860d" value="0.0"/>
                                              
